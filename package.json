{
  "name": "apathia",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:apathia1220/apathia-vue.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm build:core",
    "build:core": "pnpm -r --filter !@apathia/apathia.example --filter !@apathia/vitepress-theme --filter @apathia/apathia.* exec vite build -c ../../build/vite.build.js",
    "build:style": "pnpm --filter @apathia/apathia exec vite build -c ../../build/vite.style.js",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "createNew": "node scripts/create.js",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.org/",
    "lint": "eslint --no-fix --ext .jsx,.js,.ts,.tsx,.vue packages example scripts",
    "lint-fix": "eslint --fix --ext .jsx,.js,.ts,.tsx,.vue packages example scripts",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint",
    "postinstall": "husky install",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "lint-staged": {
    "*.{ts,vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "typescript",
    "component"
  ],
  "author": "xzc",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.24.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^1.1.4",
    "@vue/babel-plugin-jsx": "^1.0.5",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "commitlint": "^17.0.3",
    "consola": "^2.15.3",
    "eslint": "^7.25.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.20.0",
    "fs-extra": "^9.1.0",
    "husky": "^8.0.1",
    "inquirer": "8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.54.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitepress": "0.22.3",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.4"
  }
}
